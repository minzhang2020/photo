.list
  overflow: hidden

  &-item
    float: left
    width: 25%
    margin-bottom: 30px
    padding: 20px 10px
    box-sizing: border-box

    &__imgwrapper
      position: relative
      padding-top: (184 * 100 / 276) + '%'

      img
        position: absolute
        top: 0
        left: 0
        width: 100%
        height: 100%

    &__label
      line-height: 40px
      font-size: 16px
      text-align: left
      text-overflow: ellipsis
      white-space: nowrap
      overflow: hidden
